{
  "title": "EcoFlow API Test",
  "panels": [
    {
      "type": "gauge",
      "title": "Battery SOC",
      "targets": [
        { "expr": "ecoflow_api_battery_soc" }
      ]
    },
    {
      "type": "time_series",
      "title": "Input Power",
      "targets": [
        { "expr": "ecoflow_api_input_power_w" }
      ]
    },
    {
      "type": "time_series",
      "title": "Output Power",
      "targets": [
        { "expr": "ecoflow_api_output_power_w" }
      ]
    }
  ],
  "time": { "from": "now-6h", "to": "now" }
}
