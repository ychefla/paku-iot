      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "paku-pg"
          },
          "format": "table",
          "rawSql": "SELECT\n  ts,\n  topic,\n  CAST(payload->>'sensor_id' AS TEXT) AS sensor_id,\n  CAST(payload->>'temperature_c' AS NUMERIC) AS temperature_c,\n  CAST(payload->>'humidity_percent' AS NUMERIC) AS humidity_percent,\n  CAST(payload->>'pressure_hpa' AS NUMERIC) AS pressure_hpa,\n  CAST(payload->>'battery_mv' AS INTEGER) AS battery_mv\nFROM measurements\nWHERE $__timeFilter(ts)\nORDER BY ts DESC\nLIMIT 50",
