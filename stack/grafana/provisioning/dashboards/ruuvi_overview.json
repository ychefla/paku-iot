      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "paku-pg"
          },
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  CAST(payload->>'temperature_c' AS NUMERIC) AS \"temperature\"\nFROM measurements\nWHERE\n  $__timeFilter(ts)\n  AND payload ? 'temperature_c'\nORDER BY ts",
          "editorMode": "code"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
