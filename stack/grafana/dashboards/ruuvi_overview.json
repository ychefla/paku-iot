            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          },
          "format": "time_series",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  $__timeGroup(ts, $__interval) AS \"time\",\n  AVG(COALESCE((raw_data->>'inv.outTemp')::numeric, 0)) AS value,\n  'EcoFlow Inverter' AS metric\nFROM ecoflow_measurements\nWHERE\n  $__timeFilter(ts)\n  AND raw_data IS NOT NULL\n  AND (raw_data->>'inv.outTemp') IS NOT NULL\nGROUP BY 1\nORDER BY 1",
