    {
      "id": 5,
      "type": "timeseries",
      "title": "Absolute Humidity (g/m³)",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
