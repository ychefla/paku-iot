    {
      "id": 4,
      "type": "timeseries",
      "title": "Absolute Humidity (g/m³)",
      "datasource": {
        "type": "postgres",
        "uid": "paku-pg"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "paku-pg"
          },
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  -- Absolute humidity formula: AH = (RH * SVP * 2.1674) / (273.15 + T)\n  -- where SVP = 6.112 * exp((17.67 * T) / (T + 243.5))\n  ((metrics->>'humidity_percent')::numeric / 100.0 * \n   6.112 * exp((17.67 * (metrics->>'temperature_c')::numeric) / ((metrics->>'temperature_c')::numeric + 243.5)) * \n   2.1674) / (273.15 + (metrics->>'temperature_c')::numeric) AS value,\n  COALESCE(location, device_id) AS metric\nFROM measurements\nWHERE\n  $__timeFilter(ts)\n  AND site_id = 'paku'\n  AND system = 'sensors'\n  AND metrics ? 'temperature_c'\n  AND metrics ? 'humidity_percent'\nORDER BY metric, ts ASC",
          "editorMode": "code"
        }
      ],
      "options": {
