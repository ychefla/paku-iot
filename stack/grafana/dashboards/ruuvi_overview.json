      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          },
          "format": "time_series",
          "rawSql": "SELECT\n  $__timeGroup(ts, $__interval) AS \"time\",\n  AVG((metrics->>'temperature_c')::numeric) AS value,\n  device_id || ' (' || COALESCE(location, 'unknown') || ')' AS metric\nFROM measurements\nWHERE\n  $__timeFilter(ts)\n  AND site_id = 'paku'\n  AND system = 'sensors'\n  AND metrics ? 'temperature_c'\n  AND ts >= '2020-01-01'\nGROUP BY 1, device_id, location\nORDER BY 1",
          "editorMode": "code",
          "rawQuery": true
        },
        {
