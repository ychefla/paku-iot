    {
      "id": 1,
      "type": "timeseries",
      "title": "Temperature (\u00b0C)",
      "datasource": {
        "type": "postgres",
        "uid": "paku-pg"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "paku-pg"
          },
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  (metrics->>'temperature_c')::numeric AS value,\n  COALESCE(location, device_id) || ' Temp' AS metric\nFROM measurements\nWHERE\n  $__timeFilter(ts)\n  AND site_id = 'paku'\n  AND system = 'sensors'\n  AND metrics ? 'temperature_c'\nORDER BY metric, ts ASC",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "datasource": {
            "type": "postgres",
            "uid": "paku-pg"
          },
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  -- Dew point formula using Magnus-Tetens approximation\n  (metrics->>'temperature_c')::numeric - \n  ((100 - (metrics->>'humidity_percent')::numeric) / 5.0) AS value,\n  COALESCE(location, device_id) || ' Dew Point' AS metric\nFROM measurements\nWHERE\n  $__timeFilter(ts)\n  AND site_id = 'paku'\n  AND system = 'sensors'\n  AND metrics ? 'temperature_c'\n  AND metrics ? 'humidity_percent'\nORDER BY metric, ts ASC",
          "editorMode": "code"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
