          "rawQuery": true,
          "rawSql": "SELECT\n  $__timeGroup(ts, $__interval) AS \"time\",\n  AVG(soc_percent) AS value,\n  'Battery SOC (%)' AS metric\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\n  AND $__timeFilter(ts)\nGROUP BY 1\nUNION ALL\nSELECT\n  $__timeGroup(ts, $__interval) AS \"time\",\n  AVG(remain_time_min) / 60.0 AS value,\n  'Runtime (hours)' AS metric\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\n  AND $__timeFilter(ts)\n  AND remain_time_min IS NOT NULL\nGROUP BY 1\nORDER BY 1",
