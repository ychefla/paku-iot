{
  "uid": "paku-ecoflow",
  "title": "EcoFlow Power Station Overview",
  "tags": ["paku", "ecoflow", "power"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "device_sn",
        "type": "query",
        "label": "Device",
        "datasource": {
          "type": "postgres",
          "uid": "DS_POSTGRESQL"
        },
        "query": "SELECT DISTINCT device_sn FROM ecoflow_measurements WHERE device_sn != '' ORDER BY device_sn",
        "refresh": 1,
        "multi": false,
        "includeAll": false
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Battery Level",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  soc_percent\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\n  AND soc_percent IS NOT NULL\nORDER BY ts DESC\nLIMIT 1",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 20,
                "color": "orange"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Current Input Power",
      "datasource": {
