        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  remain_time_min\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\n  AND remain_time_min IS NOT NULL\nORDER BY ts DESC\nLIMIT 1",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
