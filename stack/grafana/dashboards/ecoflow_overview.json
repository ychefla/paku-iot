          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 60,
                "color": "orange"
              },
              {
                "value": 180,
                "color": "yellow"
              },
              {
                "value": 360,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Solar Input",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  COALESCE(pv_in_watts, 0) as pv_in_watts\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\nORDER BY ts DESC\nLIMIT 1",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "gray"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 100,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Battery Level Over Time",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  soc_percent AS \"Battery %\"\nFROM ecoflow_measurements\nWHERE\n  device_sn = '$device_sn'\n  AND soc_percent IS NOT NULL\n  AND $__timeFilter(ts)\nORDER BY ts ASC",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Power In/Out",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  COALESCE(watts_in_sum, 0) AS \"Input (W)\",\n  COALESCE(watts_out_sum, 0) AS \"Output (W)\"\nFROM ecoflow_measurements\nWHERE\n  device_sn = '$device_sn'\n  AND $__timeFilter(ts)\nORDER BY ts ASC",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Input (W)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Output (W)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Solar Input Over Time",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  COALESCE(pv_in_watts, 0) AS \"Solar Input (W)\"\nFROM ecoflow_measurements\nWHERE\n  device_sn = '$device_sn'\n  AND $__timeFilter(ts)\nORDER BY ts ASC",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "fixed",
            "fixedColor": "yellow"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Output Breakdown",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  COALESCE(ac_out_watts, 0) AS \"AC Output (W)\",\n  COALESCE(dc_out_watts, 0) AS \"DC Output (W)\",\n  COALESCE(typec_out_watts, 0) AS \"USB-C (W)\",\n  COALESCE(usb_out_watts, 0) AS \"USB-A (W)\"\nFROM ecoflow_measurements\nWHERE\n  device_sn = '$device_sn'\n  AND $__timeFilter(ts)\nORDER BY ts ASC",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Net Power (Input - Output)",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  ts AS \"time\",\n  (COALESCE(watts_in_sum, 0) - COALESCE(watts_out_sum, 0)) AS \"Net Power (W)\"\nFROM ecoflow_measurements\nWHERE\n  device_sn = '$device_sn'\n  AND $__timeFilter(ts)\nORDER BY ts ASC",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": -2000,
                "color": "red"
              },
              {
                "value": -500,
                "color": "orange"
              },
              {
                "value": 0,
                "color": "yellow"
              },
              {
                "value": 100,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Power Statistics (Last Hour)",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 22
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  ROUND(AVG(COALESCE(watts_out_sum, 0))) AS \"Avg Output\",\n  MAX(COALESCE(watts_out_sum, 0)) AS \"Peak Output\",\n  ROUND(AVG(COALESCE(watts_in_sum, 0))) AS \"Avg Input\",\n  MAX(COALESCE(watts_in_sum, 0)) AS \"Peak Input\"\nFROM ecoflow_measurements\nWHERE\n  device_sn = '$device_sn'\n  AND ts >= NOW() - INTERVAL '1 hour'",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1000,
                "color": "yellow"
              },
              {
                "value": 2000,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "none",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 12,
      "type": "table",
      "title": "Recent Measurements",
      "datasource": {
        "type": "postgres",
        "uid": "DS_POSTGRESQL"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 30
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  ts AS \"Time\",\n  soc_percent AS \"Battery %\",\n  watts_in_sum AS \"Input (W)\",\n  watts_out_sum AS \"Output (W)\",\n  pv_in_watts AS \"Solar (W)\",\n  remain_time_min AS \"Remain (min)\",\n  ac_out_watts AS \"AC Out (W)\",\n  dc_out_watts AS \"DC Out (W)\",\n  typec_out_watts AS \"USB-C (W)\",\n  usb_out_watts AS \"USB-A (W)\"\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\nORDER BY ts DESC\nLIMIT 20",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": []
      }
    }
  ]
}
