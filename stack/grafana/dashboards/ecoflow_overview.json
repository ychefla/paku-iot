      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  watts_out_sum\nFROM ecoflow_measurements\nWHERE device_sn = '$device_sn'\n  AND watts_out_sum IS NOT NULL\nORDER BY ts DESC\nLIMIT 1",
          "datasource": {
            "type": "postgres",
            "uid": "DS_POSTGRESQL"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "thresholds"
