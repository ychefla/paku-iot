      ],
      "fieldConfig": {
        "defaults": {
          "unit": "watt",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "blue"
              },
              {
                "value": 100,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
